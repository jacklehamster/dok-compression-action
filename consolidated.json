{
  "package.json": {
    "name": "github-actions-consolidate-json",
    "version": "1.0.1",
    "scripts": {
      "build": "tsc"
    },
    "devDependencies": {
      "typescript": "^4.5.2"
    }
  },
  "tsconfig.json": {
    "compilerOptions": {
      "lib": [
        "ESNext"
      ],
      "target": "ESNext",
      "module": "ESNext",
      "moduleDetection": "force",
      "jsx": "react-jsx",
      "allowJs": true,
      "moduleResolution": "node",
      "noEmit": true,
      "skipLibCheck": true,
      "strict": true,
      "noFallthroughCasesInSwitch": true,
      "forceConsistentCasingInFileNames": true
    }
  }
}